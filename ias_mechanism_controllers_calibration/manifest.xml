<package>
  <description brief="ias_mechanism_controllers_calibration">

     Calibration tools for the controllers in the ias_mechanism_controllers
     package. Most importantly, some utilities to tweak the gains
     the pr2_multi_vel controller.

  </description>
  <author>Georg Bartels</author>
  <license>BSD</license>
  <review status="unreviewed" notes="" />

  <depend package="roscpp" />
  <depend package="std_msgs" />
  <depend package="ias_mechanism_controllers" />
  <depend package="actionlib" />
  <depend package="pr2_controllers_msgs" />
  <depend package="pr2_mechanism_msgs" />
  <depend package="trajectory_msgs" />
  <depend package="pr2_gazebo" />
  <depend package="rxtools" />

  <export>
   <cpp cflags="-I${prefix}/include" lflags="-L${prefix}/lib -lias_mechanism_controllers_calibration"/>
  </export>

</package>


